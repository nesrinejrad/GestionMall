{% extends 'MyAppUserBundle:Default:base.html.twig' %}

{% block body %}
    <h1>Boutique</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ boutique.id }}</td>
            </tr>
            <tr>
                <th>Nomboutiques</th>
                <td>{{ boutique.nomBoutiques }}</td>
            </tr>
            <tr>
                <th>Categorieboutique</th>
                <td>{{ boutique.categorieBoutique }}</td>
            </tr>
            <tr>
                <th>Descriptionboutique</th>
                <td>{{ boutique.descriptionBoutique }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('boutiques_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('boutiques_edit', { 'id': boutique.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
